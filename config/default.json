{
  "host": "localhost",
  "port": 3000,
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "log": {
    "level": "debug"
  },
  "auth": {
    "path": "/auth",
    "secret": "!__supersecret__!",
    "jwt": {
      "issuer": "my_auth",
      "audience": "my_auth",
      "expiresIn": "1h"
    }
  },
  "local": {
    "usernameField": "identifier"
  },
  "db": {
    "url": "mongodb://localhost:27017/my_auth"
  }
}
