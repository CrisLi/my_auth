{
  "host": "localhost",
  "port": 3000,
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "log": {
    "level": "debug"
  },
  "auth": {
    "path": "/auth",
    "secret": "!__supersecret__!",
    "token": {
      "payload": ["org", "roles"]
    }
  },
  "jwt": {
    "iss": "my_auth",
    "aud": "my_auth"
  },
  "local": {
    "usernameField": "identifier"
  }
}
